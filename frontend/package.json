{
  "name": "listner-app",
  "version": "1.0.0",
  "homepage": "https://github.com/danilovmaximov/lister",
  "bugs": {
    "url": "https://github.com/danilovmaximov/lister/issues"
  },
  "author": "Ningaro (https://github.com/ningaro)",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx && tsc --noEmit",
    "style": "prettier . --write",
    "extract-lang": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file src/lang/raw/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile-lang": "formatjs compile",
    "compile-lang-all": "node scripts/compileAllLangs.mjs",
    "preview": "vite preview"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "axios": "^1.7.4",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-intl": "^6.6.8",
    "react-router-dom": "^6.23.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node"
    }
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
